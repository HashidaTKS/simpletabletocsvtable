{
	"name": "simpletabletocsvtable",
	"displayName": "SimpleTableToCsvTable",
	"description": "convert a simple table to a csv table",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:simpletabletocsvtable.helloworld",
		"onCommand:simpletabletocsvtable.convert"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "simpletabletocsvtable.convert",
				"title": "Convert Simple Table To Csv Table"
			}],
		"menus": {
			"editor/context": [{
				"command": "simpletabletocsvtable.convert",
				"group": "SimpleTableConvertor@1"
		  }]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"csv": "^5.3.2",
		"iconv-lite": "^0.5.1"
	}
}
